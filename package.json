{
  "name": "angular-playground",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js",
    "watch": "ng build --watch --configuration development",
    "watchQuick": "ng build --source-map=false --watch --configuration development",
    "buildAsProduction": "ng build --configuration production",
    "startAsProduction": "ng serve --configuration production",
    "buildAsProductionWithSourceMaps": "npm run build -- --configuration production --source-map=true",
    "test": "ng test --browsers ChromeHeadless",
    "web": "ng serve --hmr --port 4201",
    "updatePackagesMinor": "ncu -u -f /@venios.*/ --deep --target minor",
    "serve": "concurrently \"npm run web\" \"npm run test\""
  },
  "dependencies": {
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@nrwl/angular": "^15.4.1",
    "ajv": "^8.12.0",
    "ngx-indexed-db": "^16.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "uuid": "^9.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.0",
    "@angular-eslint/eslint-plugin": "^15.2.0",
    "@angular-eslint/eslint-plugin-template": "^15.2.0",
    "@angular-eslint/template-parser": "^15.2.0",
    "@angular/cli": "^15.2.0",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/language-service": "^15.2.0",
    "@nrwl/cypress": "15.4.1",
    "@nrwl/eslint-plugin-nx": "15.4.1",
    "@nrwl/jest": "15.4.1",
    "@nrwl/linter": "15.4.1",
    "@nrwl/workspace": "15.4.1",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cypress": "^11.0.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.3",
    "nx": "15.4.1",
    "prettier": "^2.6.2",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
